<!DOCTYPE html>
<html lang="en">
  <body>
    <h1>Actualizar persona</h1>

    <form action=" /updatePerson" method="POST">
      <label for="nss">NSS:</label>
      <input
        type="text"
        id="nss"
        name="nss"
        value="<%= `${boludo.nss}` %>"
      /><br /><br />
      <label for="nombre">Nombre:</label>
      <input
        type="text"
        id="nombre"
        name="nombre"
        value="<%= `${boludo.nombre}` %>"
      /><br /><br />
      <label for="edad">Edad:</label>
      <input
        type="text"
        id="edad"
        name="edad"
        value="<%= `${boludo.edad}` %>"
      /><br /><br />
      <label for="tipoSangre">Tipo de Sangre:</label>
      <input
        type="text"
        id="tipoSangre"
        name="tipoSangre"
        value="<%= `${boludo.tipoSangre}` %>"
      /><br /><br />
      <label for="objId">ID:</label>
      <input
        type="text"
        id="objId"
        name="objId"
        value="<%= `${boludo._id}` %>"
      /><br /><br />
      <input type="submit" value="submit" />
    </form>
    <a href="/">Regresar</a><br />
  </body>
</html>
